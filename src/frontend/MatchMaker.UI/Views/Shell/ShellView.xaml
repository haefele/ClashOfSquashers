<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MatchMaker.UI.Views.Shell.ShellView"
             xmlns:pages="clr-namespace:MatchMaker.UI.Views.Shell"
                  xmlns:main="clr-namespace:MatchMaker.UI.Views.Main">
    <MasterDetailPage.Master>
        <pages:ShellViewMaster x:Name="MasterPage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <main:MainView />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>